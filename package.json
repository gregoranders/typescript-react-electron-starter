{
  "name": "typescript-react-electron-starter",
  "version": "0.0.9",
  "description": "TypeScript React Electron Starter",
  "main": "app/main.js",
  "scripts": {
    "clear": "gulp --cwd . clear && rimraf ./node_modules ./app ./dist ./coverage ./coverage-it ./it/screenshots ./package-lock.json ./gulpfile.js ./gulpfile.js.map ./.github/actions/gulpfile.js ./.github/actions/gulpfile.js.map",
    "postinstall": "tsc -p ./",
    "actions": "gulp --gulpfile ./.github/actions/gulpfile.js --cwd .",
    "prebuild": "rimraf ./app ./dist ./coverage ./coverage-it ./it/screenshots",
    "build": "gulp --cwd .",
    "predist": "rimraf ./app ./dist",
    "dist": "gulp --cwd . dist",
    "start": "electron ./",
    "pretest": "rimraf ./coverage",
    "test": "jest --testEnvironment enzyme --coverage --coverageDirectory ../coverage --rootDir ./src",
    "preit": "rimraf ./coverage-it ./it/screenshots",
    "it": "jest --runInBand --coverage --coverageDirectory ../coverage-it --rootDir ./it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gregoranders/typescript-react-electron-starter.git"
  },
  "keywords": [
    "electron",
    "gulp",
    "jest",
    "react",
    "scss",
    "typescript"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!main.ts",
      "!renderer.tsx"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "reporters": [
      "jest-spec-reporter"
    ],
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "transform": {
      "\\.ts(x)?$": "ts-jest"
    }
  },
  "author": "Gregor Anders <gregor.anders@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gregoranders/typescript-react-electron-starter/issues"
  },
  "homepage": "https://gregoranders.github.io/typescript-react-electron-starter/",
  "dependencies": {
    "electron": "9.0.5",
    "fancy-log": "1.3.3",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@actions/core": "1.2.4",
    "@actions/github": "3.0.0",
    "@octokit/core": "3.1.0",
    "@octokit/rest": "18.0.0",
    "@octokit/types": "5.0.1",
    "@types/archiver": "3.1.0",
    "@types/electron-packager": "14.0.0",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/fancy-log": "1.3.1",
    "@types/gulp": "4.0.6",
    "@types/gulp-sass": "4.0.0",
    "@types/gulp-sourcemaps": "0.0.32",
    "@types/jest": "26.0.3",
    "@types/mime": "2.0.2",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "@types/react-test-renderer": "16.9.2",
    "@types/through2": "2.0.36",
    "@types/webdriverio": "4.13.3",
    "@zeit/ncc": "0.22.3",
    "archiver": "4.0.1",
    "del": "5.1.0",
    "electron-packager": "14.2.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "gulp": "4.0.2",
    "gulp-git": "2.10.1",
    "gulp-jest": "4.0.3",
    "gulp-sass": "4.1.0",
    "gulp-sass-lint": "1.4.0",
    "gulp-sourcemaps": "2.6.5",
    "gulp-tslint": "8.1.4",
    "gulp-typescript": "6.0.0-alpha.1",
    "jest": "26.1.0",
    "jest-cli": "26.1.0",
    "jest-environment-enzyme": "7.1.2",
    "jest-enzyme": "7.1.2",
    "jest-spec-reporter": "1.0.14",
    "mime": "2.4.6",
    "node-sass": "4.14.1",
    "react-test-renderer": "16.13.1",
    "rimraf": "3.0.2",
    "spectre.css": "0.5.8",
    "spectron": "11.0.0",
    "through2": "3.0.2",
    "ts-jest": "26.1.1",
    "tslint": "6.1.2",
    "typescript": "3.9.5"
  }
}
